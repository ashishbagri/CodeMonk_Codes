package june_easy;

import java.util.Scanner;

public class Stevie {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int nums = sc.nextInt();
		int[] a = new int[nums];
		int[] b = new int[nums];
		int[] c = new int[nums];
		for(int i =0;i<nums;i++){
			a[i] = sc.nextInt();
		}
		for(int i =0;i<nums;i++){
			b[i] = sc.nextInt();
		}
		for(int i =0;i<nums;i++){
			for(int j=0;j<nums;j++){
				if(a[i]==a[j]){
					c[j]=b[i]>b[j]?b[i]:b[j];
				}
			}
		}
		for(int i =0;i<nums;i++){
			System.out.println(c[i]+" ");
		}
	}
}
