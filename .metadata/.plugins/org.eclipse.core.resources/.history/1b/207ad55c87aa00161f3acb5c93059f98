import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.Scanner;
import java.util.Stack;

public class MonkAndPhilospherStone {
	private static BufferedReader br = null;
	
	static{
		//sc = new Scanner(System.in);
		br = new BufferedReader(new InputStreamReader(System.in));
	}
	public static void main(String[] args) {
		int numberOfCoins = Integer.parseInt(br.readLine());
		String[] coins = br.readLine().split(" ");
		int[] coinsValue = new int[numberOfCoins];
		for(int i=0;i<numberOfCoins;i++){
			coinsValue[i] = sc.nextInt();
		}
		int numberOfInstructions = sc.nextInt();
		int x = sc.nextInt();
		
		Stack<Integer> monksBag = new Stack<Integer>();
		int harrysBagPointer = 0;
		for(int i=0;i<numberOfInstructions;i++){
			String instruction = sc.nextLine();
			
			if(instruction.equals("Harry")){
				monksBag.push(coinsValue[harrysBagPointer++]);
			}else{
				monksBag.pop();
			}
		}
		System.out.println(monksBag.size());
	}
}
