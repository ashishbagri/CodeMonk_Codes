import java.util.Scanner;


public class DexterplayswithGP {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t-->0){
			int r = sc.nextInt();
			int s = sc.nextInt();
			int p =sc.nextInt();
			int ans = -1;
			int mod =r;
			for(int i=2;i<=p;i++){
				mod = mod*r;
				if(s==mod%p){
					ans=i;
					break;
				}
				//r=mod;
					
			}
			System.out.println(ans);
		}
	}
}
