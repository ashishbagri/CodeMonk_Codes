import java.util.Scanner;

public class Stevie {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int nums = sc.nextInt();
		long[] a = new long[nums];
		long[] b = new long[nums];
		long[] c = new long[nums];
		for(long i =0;i<nums;i++){
			a[i] = sc.nextLong();
		}
		for(long i =0;i<nums;i++){
			b[i] = sc.nextLong();
		}
		for(long i =0;i<nums;i++){
			for(long j=0;j<nums;j++){
				if(a[i]==a[j]){
					c[j]=b[i]>b[j]?b[i]:b[j];
				}
			}
		}
		for(long i =0;i<nums;i++){
			System.out.prlong(c[i]+" ");
		}
	}
}
