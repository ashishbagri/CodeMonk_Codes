import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.Stack;

public class MonkAndPhilospherStone {
	private static BufferedReader br = null;
	
	static{
		//sc = new Scanner(System.in);
		br = new BufferedReader(new InputStreamReader(System.in));
	}
	public static void main(String[] args) throws Exception{
		int numberOfCoins = Integer.parseInt(br.readLine());
		String[] coins = br.readLine().split(" ");
		int[] coinsValue = new int[numberOfCoins];
		for(int i=0;i<numberOfCoins;i++){
			coinsValue[i] = Integer.parseInt(coins[i]);
		}
		String[] values = br.readLine().split(" ");
		int numberOfInstructions = Integer.parseInt(values[0]);
		int x = Integer.parseInt(values[1]);
		
		Stack<Integer> monksBag = new Stack<Integer>();
		int sumOfMonkCoins = 0;
		int harrysBagPointer = 0;
		for(int i=0;i<numberOfInstructions;i++){
			String instruction = br.readLine();
			
			if(instruction.equals("Harry")){
				int val = coinsValue[harrysBagPointer++];
				monksBag.push(val);
				sumOfMonkCoins+=val;
			}else{
				
				int r = monksBag.pop();
				sumOfMonkCoins-=r;
			}
			if(sumOfMonkCoins==x){
				break;
			}
		}
		System.out.println(monksBag.size());
	}
}
