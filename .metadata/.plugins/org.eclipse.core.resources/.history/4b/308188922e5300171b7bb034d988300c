import java.util.Scanner;


public class DexterplayswithGP {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t-->0){
			long r = sc.nextInt();
			long s = sc.nextInt();
			long p =sc.nextLong();
			int ans = -1;
			long n =1;
			for(int i=1;i<p;i++){
				n = n+(int)Math.pow(r%p, i)%p;
				if(n%p==s){
					ans=i+1;
					break;
				}
			}
			System.out.println(ans);
		}
	}
}
